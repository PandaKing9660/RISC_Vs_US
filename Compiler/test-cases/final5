// 3x3 matrix multiplication
void main(){
	
	int a[9];
	int b[9];

	output("Enter the contents of matrix 1:\n");

	int i;

	for(i=0; i<9; i=i+1){
		int t;
		input(t);
		a[i] = t;
	}

	output("Enter the contents of matrix 2:\n");
	
	for(i=0; i<9; i=i+1){
		int t;
		input(t);
		b[i] = t;
	}

	int c[9];
    
	int row;
	for(row=0; row<3; row=row+1){
		int col;
		for(col=0; col<3; col=col+1){
			int sum = 0;
			int k;
			for(k=0; k<3; k=k+1){
				sum = sum + a[row*3+k]*b[k*3+col];
			}
			c[row*3+col] = sum;
		}
	}
    

	for(i=0; i<9; i=i+1){
		output(c[i]);
		if((i+1)%3 == 0){
			output("\n");
		}else{
			output("Â ");
		}
	}
}