int factorial(int n){
    if(n==0){
        return 1;
    }
    else{
        return factorial(n-1) + n;
    }
}

int main(){

    int n;
    output("Enter n: ");
    input(n);

    int res = factorial(n);

    output(res);
    output("\n");

    return 0;
}