// Check whether it's a prime number 
int main(){

	int number = 97;

    output(number);
    output("\n");
    
    int i;
    for (i = 2; i < number; i=i+1) {
        if (number % i != 0){
            continue;
        }
        else{
            output("It is not a prime number.\n");
			return 0;
		}
    }
	output("It is a prime number.\n");

    return 0;
}
